#include <stdio.h>

/**
 * main - Entry point
 * Description: 'this is a file comtaining a function called _isupper()'
 * @c >>> a inputed parameter caontaing a character
 * Return: Always 0 (Success)
 */

int main (void)
{
	int c, lp, i;
	long n;

	n = 612852475143;
	c = n / 2;
	lp = 3;
	i = 5;
	printf("%d", c);
	while (i < c)
	{
		if (i % 2 == 0)
		{
			++i;
			printf("even :%d", i);
		}
			else if(n % i == 0)
			{	
				if (i % 3 == 0 || i % 5 == 0)
				{
					++i;
					printf("odd:%d",i);
				}
				else
				{
					if (lp < i)
					{
						lp = i;
						++i;
						printf("new lp found");
					}
				}
			}
	}	
	printf("%d", lp);
	return (0);
}
